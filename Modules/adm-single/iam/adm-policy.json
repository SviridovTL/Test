{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": [
        "cloudwatch:PutMetricData",
        "ec2:CreateSnapshot",
        "ec2:DeleteSnapshot",
        "ec2:CreateTags",
        "ec2:ModifySnapshotAttribute",
        "ec2:ResetSnapshotAttribute",
        "ec2:Describe*",
        "ec2:RebootInstances",
        "ec2:StopInstance",
        "ec2:TerminateInstances",
        "ec2:RunInstances",
        "s3:*"
      ],
      "Resource": "*",
      "Effect": "Allow",
      "Sid": "AllowAWSLogs"
    }
  ]
}
